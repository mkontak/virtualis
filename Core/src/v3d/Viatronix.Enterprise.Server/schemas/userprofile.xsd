<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" >
  <xs:include schemaLocation=".\sdk.xsd" />

  <xs:element name="userprofile">

    <xs:complexType>
      <xs:sequence>
        <xs:element name="profile"        minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
      
      <xs:attribute name="login"    type="xs:string" use="optional" />
      <xs:attribute name="name"    type="xs:string" use="optional" />
      <xs:attribute name="ver"    type="xs:string"  use="required" />

    </xs:complexType>
  </xs:element>
</xs:schema>