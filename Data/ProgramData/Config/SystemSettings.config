<?xml version="1.0" encoding="utf-8"?>
<systemSettings>
  <processor>True</processor>
  <viewer>True</viewer>
  <logLevel>Debug</logLevel>
  <configDirectory>$(APPDATA)\Viatronix\Config\</configDirectory>
  <servicesDirectory>$(InstallDirectory)\Services\</servicesDirectory>
  <usersDirectory>$(InstallDirectory)\Users\</usersDirectory>
  <viewersDirectory>$(InstallDirectory)\Viewers\</viewersDirectory>
  <processorDirectory>$(InstallDirectory)\Processing\</processorDirectory>
  <utilitiesDirectory>$(InstallDirectory)\Utilities\</utilitiesDirectory>
  <comDirectory>$(InstallDirectory)\COM\</comDirectory>
  <smtpSever></smtpSever>
  <administratorEmailAddress></administratorEmailAddress>
  <administratorEmailLevel>Exception</administratorEmailLevel>
  <supportEmailAddress></supportEmailAddress>
  <supportEmailLevel>Exception</supportEmailLevel>
  <cacheBasePath>D:\vxStudies</cacheBasePath>
  <diskWarningThreshold>75</diskWarningThreshold>
  <diskErrorThreshold>90</diskErrorThreshold>
  <cacheServer>localhost</cacheServer>
  <tempDirectory>C:\Temp</tempDirectory>
  <installDirectory>C:\Program Files\Viatronix\</installDirectory>
  <logDirectory>$(APPDATA)\Viatronix\Logs\</logDirectory>
</systemSettings>
