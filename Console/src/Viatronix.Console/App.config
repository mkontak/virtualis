    <add key="ProductId" value="V3D Console"/>
    <add key="RenewInterval" value="30"/> <!-- Value must be greater than 2. -->
    <add key="IdleTimeout" value="300"/>
    <add key="RootDirectory" value="C:\testData"/>
    <add key="IConsolePort" value="1234"/>
    <add key="LaunchRules" value="config\LaunchRules.xml"/>
    <add key="IdleInterval" value="30"/>
    <add key="AETitle" value="V3D"/>
    <add key="AllowedProcessSeriesTypes" value="Dicom"/>
    <add key="PacsQueueUrl" value="tcp://localhost:9100/PacsQueueServer.rem"/>
    <add key="HelpFile" value="V3D-Console.chm"/>
    <add key="DiskMonitorInterval" value="10"/>
    <add key="DiskMonitorDisplayDriveSystem" value="true"/>
    <add key="Importer" value="$(UtilitiesDirectory)\Importing\Importer.exe"/>
    <add key="ImporterArgs" value="zhSNRP892d26tkKSFUrPt/t9HyZTGk8eOLgfq1o8ojQ="/>
    <add key="VCViewerPath" value="$(ViewersDirectory)\VC\V3DViewerColon.exe"/>
    <add key="ShowDicomSave" value="True"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
    <add key="SupportsEncapsulatedPDF" value="false"/>
    <add key="FileSystemTransferDefaultPath" value="D:\VXFILETRANSFER\"/>
    <add key="connectionStrings" value="$(APPDATA)\Viatronix\Config\connections.config"/>
  </appSettings>
  
