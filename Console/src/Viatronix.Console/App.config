            </additional_data>
          </view>
          <view id="PacsQueue" text="PACS Queue" description="" typename="Viatronix.Console.ViewStatusPacs, Console" version="1.0">
            <additional_data>
              <ServerServices version="1.0">
                <listviews pacsQueueService="pacsQueueColumns"/>
                <PacsQueueServices RefreshRate="30">
                  <url address="tcp://localhost:9100/PacsQueueServer.rem"/>
                </PacsQueueServices>
              </ServerServices>
            </additional_data>
          </view>
        </views>
      </tab>
      <tab id="Users and Groups" text="User Management" default_view="UsersView" version="1.0">
        <menus>
          <menu text="File" typename="Viatronix.Console.MenuFile, Console" version="1.0"/>
          <menu text="Help" typename="Viatronix.Console.MenuHelp, Console" version="1.0"/>
        </menus>
        <panes>
        </panes>
        <views>
          <view id="UsersView" text="Users" description="" typename="Viatronix.Console.ViewUMAS, Console" version="1.0">
            <additional_data>
              <configuration dataProvider="security">
                <listviews userView="userColumns"/>
              </configuration>
            </additional_data>
          </view>
        </views>
      </tab>
    </tabs>
  </frame>
  
