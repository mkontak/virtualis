      <pane id="DetailsPane" text="Details" typename="Viatronix.Console.DetailsPane, Console" version="1.0"/>
      <pane id="CDSearchPane" text="Search" typename="Viatronix.Console.CDSearchPane, Console" version="1.0"/>
      <pane id="PACSSearchPane" text="Search" typename="Viatronix.Console.PACSSearchPane, Console" version="1.0"/>
