          <menu text="Help" typename="Viatronix.Console.MenuHelp, Console" version="1.0"/>
        </menus>
        <panes>
          <pane id="DetailsPane" expanded="true" version="1.0"/>
          <!--pane id="LaunchViewerPane" expanded="true"  version="1.0"/-->
        </panes>
        <views>
          
          <!-- Database View -->
          <view id="Default" text="Database" description="" typename="Viatronix.Console.ViewBrowser, Console" version="1.0">
            <additional_data>
              <configuration dataProvider="data" canLaunch="true" canTransfer="True" useAnimation="false" canDelete="True" canImport="true" canShowDicomFolder="true" canShowDicomInfo="true" canProcess="true" displayLaunchPane="false">
                 <transferProviders>
                  <transferProvider name="PACS" description="Transfer to PACS" typename="Viatronix.Console.PACSTransferProvider, Console" allowItemSelection="true" anonymize="false" override="true" seriesCollectionTransfer="true" version="1.0">
                    <transferItems>
                      <item subtype="vc" name="Snapshots" searchInSeries="Session" searchInFolder="" formats=".vrx,.jpg,.png" checkState="true" sessiontypename="Viatronix.Console.VCSessionProvider"/>
                      <item subtype="vc" name="Reports" searchInSeries="Session" searchInFolder="" formats=".vrx" checkState="true" sessiontypename="Viatronix.Console.VCSessionProvider"/>
                      <!--<item subtype="vc" name="Movies" searchInSeries="Session" searchInFolder="" formats=".avi,.wmv,.png,.jpg" checkState="true" sessiontypename="Viatronix.Console.VCSessionProvider" />-->
                      <item subtype="default" name="Snapshots" searchInSeries="Session" searchInFolder="Snapshots" formats=".jpg,.png" checkState="true" sessiontypename="Viatronix.Console.V3DSessionProvider"/>
                      <!--<item subtype="default" name="Movies" searchInSeries="Session" searchInFolder="Movies" formats=".avi,.wmv,.png" checkState="true" sessiontypename="Viatronix.Console.V3DSessionProvider" />-->
                      <item subtype="default" name="Reports" searchInSeries="Session" searchInFolder="Report" formats="report.xml" checkState="true" sessiontypename="Viatronix.Console.V3DSessionProvider"/>
                      <item subtype="default" name="DICOM" searchInSeries="Dicom" searchInFolder="" formats=".dcm" checkState="false" sessiontypename=""/>
                    </transferItems>
                  </transferProvider>
                  <transferProvider name="Patient CD" description="CD which you can provide to patient." typename="Viatronix.Console.RemovableMediaTransferProviderForPatient, Console" allowItemSelection="true" anonymize="false" override="false" seriesCollectionTransfer="true" version="1.0">
                    <transferItems>
                      <item subtype="vc" name="Snapshots" searchInSeries="Session" searchInFolder="" formats=".vrx,.jpg,.png,.jpg" checkState="true" sessiontypename="Viatronix.Console.VCSessionProvider"/>
                      <item subtype="vc" name="Reports" searchInSeries="Session" searchInFolder="" formats=".vrx" checkState="true" sessiontypename="Viatronix.Console.VCSessionProvider"/>
                      <item subtype="vc" name="Movies" searchInSeries="Session" searchInFolder="" formats=".avi,.wmv,.png,.jpg" checkState="true" sessiontypename="Viatronix.Console.VCSessionProvider"/>
                      <item subtype="default" name="Snapshots" searchInSeries="Session" searchInFolder="Snapshots" formats=".jpg,.png" checkState="true" sessiontypename="Viatronix.Console.V3DSessionProvider"/>
                      <item subtype="default" name="Movies" searchInSeries="Session" searchInFolder="Movies" formats=".avi,.wmv,.png" checkState="true" sessiontypename="Viatronix.Console.V3DSessionProvider"/>
                      <item subtype="default" name="Reports" searchInSeries="Session" searchInFolder="Report" formats="report.xml" checkState="true" sessiontypename="Viatronix.Console.V3DSessionProvider"/>
                      <item subtype="default" name="DICOM" searchInSeries="Dicom" searchInFolder="" formats=".dcm" checkState="true" sessiontypename=""/>
                    </transferItems>
                  </transferProvider>
                  <transferProvider name="Archival CD" description="CD archive." typename="Viatronix.Console.RemovableMediaTransferProviderForArchival, Console" allowItemSelection="true" anonymize="true" override="true" seriesCollectionTransfer="true" version="1.0">
                  </transferProvider>
                  <transferProvider name="File System Archival" description="Stores to the File System for Archival." typename="Viatronix.Console.FileSystemTransferProvider, Console" allowItemSelection="false" anonymize="false" override="false" seriesCollectionTransfer="true" version="1.0">
                  </transferProvider>
                </transferProviders>
                <listviews studies="studyColumns" series="seriesColumns"/>
              </configuration>
            </additional_data>
            <panes>
              <pane id="DetailsPane" expanded="true" version="1.0"/>
              <pane id="DatabaseSearchPane" expanded="true" version="1.0"/>
              <pane id="DiskMonitorPane" expanded="true" version="1.0"/>
              <!--pane id="LaunchViewerPane"   expanded="false" version="1.0"/-->
            </panes>
          </view>
          
          
            
          
          <!-- PACS Query View -->
          <view id="PACS" text="PACS" description="" typename="Viatronix.Console.ViewBrowser, Console" version="1.0">
            <additional_data>
              <configuration dataProvider="pacs" canLaunch="false" canTransfer="true" useAnimation="false" canDelete="False" canImport="false" canShowDicomFolder="false" canShowDicomInfo="false" canProcess="false">
                <transferProviders>
                  <transferProvider name="Viatronix Dicom Server" description="Push to Viatronix dicom server" typename="Viatronix.Console.DicomServerTransferProvider, Console" allowItemSelection="false" sssanonymize="false" override="false" asynchronous="false" seriesCollectionTransfer="true" version="1.0">
                    <transferParameters>
                      <transferParameter name="DicomServer0" value="tcp://localhost:8999/DicomServers.rem"/>
                    </transferParameters>
                    <transferItems>
                      <item subtype="default" name="DICOM" searchInSeries="Dicom" searchInFolder="" formats="" checkState="true" sessiontypename=""/>
                    </transferItems>
                  </transferProvider>
                </transferProviders>
                <listviews studies="studyColumns" series="seriesColumns"/>
              </configuration>
            </additional_data>
            <panes>
              <pane id="DetailsPane" expanded="false" version="1.0"/>
              <pane id="PACSSearchPane" expanded="true" version="1.0"/>
            </panes>
