    <hotkeys>
      <hotkey key="F5" typename="Viatronix.Console.HotKeySearch, Console" version="1.0"/>
      <hotkey key="F8" typename="Viatronix.Console.HotKeyTogglePanes, Console" version="1.0"/>
      <hotkey key="F1" typename="Viatronix.Console.HotKeyHelp, Console" version="1.0"/>
    </hotkeys>
    
    <!-- Panes -->
