  <!-- Custom section handlers -->
  <configSections>
    <section name="loggers" type="Viatronix.Logging.LoggersSection, v3dlog"/>
    <section name="frame" type="Viatronix.Enterprise.Configuration.XmlConfigSectionHandler, v3dent"/>
    <section name="dynamicColumns" type="Viatronix.Enterprise.Configuration.XmlConfigSectionHandler, v3dent"/>
    <section name="sessionViewers" type="Viatronix.Console.SeriesDetailsPanel, Console"/>
    <section name="studyGroups" type="Viatronix.Enterprise.Configuration.XmlConfigSectionHandler, v3dent"/>
    <section name="aboutInfo" type="Viatronix.UI.AboutConfigSectionHandler, v3dui"/>
    <section name="transferItems" type="Viatronix.Enterprise.Configuration.XmlConfigSectionHandler, v3dent"/>
    <section name="theme" type="Viatronix.Enterprise.Configuration.XmlConfigSectionHandler, v3dent"/>
    <section name="launchRules" type="Viatronix.Console.LaunchRuleConfigSectionHandler, v3dconcore"/>
    <section name="reportMacros" type="Viatronix.Console.RemovableMediaTransferProviderForPatient, Console"/>
    <section name="groupSettings" type="Viatronix.Dicom.UI.GroupSettingsSectionHandler, v3ddcmui"/>
    <section name="simpleModeRules" type="Viatronix.Console.SimpleModeRulesConfigSectionHandler, v3dconcore"/>
    <section name="providers" type="Viatronix.Enterprise.Configuration.ProviderSection, v3dent"/>
  </configSections>


  <!-- 
  ============================================================================================================================================================  
   Section:     Providers                                                                                                                                         
   Description: List all the providers used within the application. There must be al least on security provider.                                                  
                                                                                                                                                                  
   Provider Xml Attribute Defintion                                                                                                                               
                                                                                                                                                                  
   Attribute  Required    Dscription                                                                                                                              
   name         Y         Name of the provider, must be unique.                                                                                                   
   type         Y         Fully Qualified Type name. This is the type that will be created by the ProviderFactory                                                 
   url          N         Used to identify a connection point for the provider. May be left off if there is not connection point such as the Database providers   
   aetitle      N         AE Title to be used for the provoder (PACS Providers)                                                                                   
   renewal      N         Renewal time in minutes. If this attribute is present and has a value > 0 then a renewal provider will be created internally in the     
                          provider if the provider is a security provider. If the entry is absent or 0 then the renewal provider implemented in the security      
                          provider will be disabled and it will be the applications responsibility to perform the context renewal.                                
   security     N         Security provider name that the provider links to. This is not required as the first defined security provider will be used. If         
                          you want to link providers to another security provider defined in the list you may use this attribute to do so.                        
   ver          Y         Version of the xml (1.0)                                                                                                                                                                                                                                                                   
                                                                                                                                                                  
                                                                                                                                                                  
  ============================================================================================================================================================ 
  -->
  <providers>

    <!-- REST PROVIDERS -->
   
    <provider name="security" type="Viatronix.Enterprise.SDK.Providers.RestSecurityProvider, v3dent" url="http://apollo:8000/Enterprise" ver="1.0"/>
    <provider name="data" type="Viatronix.Enterprise.SDK.Providers.RestDataProvider, v3dent" url="http://apollo:8000/Enterprise" ver="1.0"/>
    <provider name="pacs" type="Viatronix.Enterprise.SDK.Providers.DirectPacsProvider, v3dent" aetitle="V3D" ver="1.0"/>
    

    <!-- DATABASE PROVIDERS/DIRECT PACS PROVIDER -->
    <!--
    <provider name="security" type="Viatronix.Enterprise.SDK.Providers.DatabaseSecurityProvider, v3dent" ver="1.0"/>
    <provider name="data" type="Viatronix.Enterprise.SDK.Providers.DatabaseDataProvider, v3dent"  ver="1.0"/>
    <provider name="pacs" type="Viatronix.Enterprise.SDK.Providers.DirectPacsProvider, v3dent" aetitle="V3D"  ver="1.0"/>
     -->
    
  </providers>


  <!-- ============================================================================================================ -->
  <!-- Logger definitions                                                                                           -->
  <!--                                                                                                              -->
  <!-- Attributes:                                                                                                  -->
  <!--                                                                                                              -->
  <!-- name     required  Unique name used to identify the logger. Will also be used as the filename's              -->
  <!--                    root of not 'file' attribute is specified.                                                -->
  <!--                                                                                                              -->
  <!-- level    required  Level of the logging (Verbose, Debug, Warning, Error )                                    -->
  <!--                                                                                                              -->
  <!-- default  required  Flag indicating the logger is the default. This allows for backward compatibility when    -->
  <!--                    the CORE libraries are used since they log only to the default. Only one logger should    -->
  <!--                    be marked as default. Igf more then one is then the last one in the list will be the      -->
  <!--                    default.                                                                                  -->
  <!--                                                                                                              -->
  <!-- file     optional  If specified identifies the file and path the logger is to use. If not specified then      -->
  <!--                    the logger will use the application data directory for the OS it is running on and        -->
  <!--                    append Viatronix\Logs\<name>.ext to it.                                                   -->
  <!--                                                                                                              -->
  <!-- ============================================================================================================ -->
  <loggers>
    <!-- server="atlas" port="2000" -->
    <logger name="Console" level="Verbose" format="Text" default="true"/>
  </loggers>

  <theme>
    <color>0,46,186</color>
    <!--backColor>0,255,255</backColor-->
    <!--font>Times New Roman</font>
    <fontSize>8</fontSize-->
  </theme>
  
   <!-- ======================================================================================================================================================= -->
   <!--                                                                                                                                                         -->
   <!--    Application Settings                                                                                                                                 -->
   <!--                                                                                                                                                         -->
   <!--     studyStateIndicator         animated      Uses animated icons at the study level to indciate state                                                  -->
   <!--                                 static        Uses non-animated icons as the study level to indicate state                                              -->
   <!--                                 none          No state is displayed (Like 3.5)                                                                          -->
   <!--                                                                                                                                                         -->
   <!--     DicomLog                    Dicom log file used to log dicom transactions generated through Console.                                                -->
   <!--                                                                                                                                                         -->
   <!--     StoreModality               Sets the modality to store images to a PACS. The default is blank indicating that the series                            -->
   <!--                                 modality is to be used. Otherwise we can specify the modality such as SC (Secondary Capture).                           -->
   <!--                                                                                                                                                         -->
   <!--     SimpleMode                  true      Displays a simpiler view of the datasets (MAY BE REMOVED)                                                     -->
   <!--                                 false     Regualar display mode                                                                                         -->
   <!--                                                                                                                                                         -->
   <!--     ProductId                   V3D Console                                                                                                             -->
   <!--                                                                                                                                                         -->
   <!--     RenewalInterval             Value in minutes that the system attempts to renew a license (must be greater then 2).                                  -->
   <!--                                                                                                                                                         -->
   <!--     IdleTimeout                 Time in minutes befire the application times out and requires a user to relogin.                                        -->
   <!--                                                                                                                                                         -->
   <!--     RootDirectory               ???                                                                                                                     -->
   <!--                                                                                                                                                         -->
   <!--     LaunchRules                 Location of the launch rules                                                                                            -->
   <!--                                                                                                                                                         -->
   <!--     AETitle                     AE Title of the console (Moved to setting in PACS Provider see providers above). This entry is still used in the        -->
   <!--                                 PACS transfer providers.                                                                                                -->
   <!--                                                                                                                                                         -->
   <!--     AllowedProcessSeriesTypes   List of series types processing is alloed to be started from (Used for reprocessing).                                   -->
   <!--                                                                                                                                                         -->
   <!--     PacsQueueUrl                 URL of the PACS queue server (Will be removed in 3.6)                                                                  -->
   <!--                                                                                                                                                         -->
   <!--     HelpFile                    Name of the Console's help file                                                                                         -->
   <!--                                                                                                                                                         -->
   <!--     DiskMonitorInterval         The refresh time in minutes for updating the disk monitor pane                                                          -->
   <!--                                                                                                                                                         -->
   <!--     DiskMonitorDisplayDriveSystem   true    Displays the drive in the usage bar                                                                         -->
   <!--                                     false   Does not display the drive                                                                                  -->
   <!--                                                                                                                                                         -->
   <!--                                                                                                                                                         -->
   <!--     Importer                     Location of the importer executable used to import data into the Console.                                              -->
   <!--                                                                                                                                                         -->
   <!--     ImportArgs                   Encrypted string of arguments used when the importer is launched.                                                      -->
   <!--                                                                                                                                                         -->
   <!--                                                                                                                                                         -->
   <!--      VCViewerPath                Location of the VC viewer executable. Used in the VCSessionProvider when the details are dispayed                      -->
   <!--                                                                                                                                                         -->
   <!--      ShowDicomSave                true      The save dicom is menu selection is available on the Pendind Jobs context menu.                             -->
   <!--                                   false     Cannot save dicom.                                                                                          -->
   <!--                                                                                                                                                         -->
   <!--                                                                                                                                                         -->
   <!--      SupportsEncapsulatedPDF      true      Encapsulated PDF is supported in DICOM                                                                      -->
   <!--                                   false     Encapsulated PDF not supported in DICOM                                                                     -->
   <!--                                                                                                                                                         -->
   <!--      FileSystemTransferDefaultPath   Temporart location used during transfers                                                                           -->
   <!--                                                                                                                                                         -->
   <!--      connectionStrings            Location of the connection database connect strings file.                                                             -->
   <!-- ======================================================================================================================================================= -->

   <appSettings>
